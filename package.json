{
  "name": "rls-auth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production prisma generate && next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:dev:generate": "dotenv -e .env.development.local -- prisma generate --schema=./prisma/schema.dev.prisma",
    "db:dev:migrate": "dotenv -e .env.development.local -- prisma migrate dev --schema=./prisma/schema.dev.prisma",
    "db:prod:migrate": "dotenv -e .env.production.local -- prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:dev:push": "NODE_ENV=development prisma db push --schema=./prisma/schema.dev.prisma",
    "db:prod:deploy": "NODE_ENV=production prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:prod:push": "NODE_ENV=production prisma db push --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@reduxjs/toolkit": "^2.5.1",
    "@stripe/stripe-js": "^5.7.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.48.1",
    "@tailwindcss/postcss": "^4.0.2",
    "@types/react": "^19.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.474.0",
    "next": "15.1.6",
    "next-themes": "^0.4.4",
    "openai": "^4.82.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.1",
    "react-redux": "^9.2.0",
    "reactflow": "^11.11.4",
    "resend": "^4.1.2",
    "stripe": "^17.7.0",
    "supabase": "^2.9.6",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.20.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.17.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.20.1",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.5.1",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "resolutions": {
    "@types/react": "^19.0.0"
  }
}
